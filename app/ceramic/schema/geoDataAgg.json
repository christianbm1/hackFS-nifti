{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "geoData",
    "description": "A geographical coordinate on a planet (most commonly Earth).",
    "type": "object",
    "properties": {
        "latitude": {
            "type": "number",
            "minimum": -90,
            "maximum": 90
        },
        "longitude": {
            "type": "number",
            "minimum": -180,
            "maximum": 180
        }
    },
    "required": [ "latitude", "longitude" ]
}